Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"CaesarDecrypt", "[", 
    RowBox[{
     RowBox[{"cipherText_String", "?", "LowerCaseQ"}], ",", "key_Integer"}], 
    "]"}], "/;", 
   RowBox[{"1", "\[LessEqual]", "key", "\[LessEqual]", "25"}]}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"az", "=", 
      RowBox[{"CharacterRange", "[", 
       RowBox[{"\"\<a\>\"", ",", "\"\<z\>\""}], "]"}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"decrypt", "=", 
      RowBox[{"ReplaceAll", "[", 
       RowBox[{"Thread", "[", 
        RowBox[{"az", "->", 
         RowBox[{"RotateRight", "[", 
          RowBox[{"az", ",", "key"}], "]"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"StringJoin", "[", 
      RowBox[{"decrypt", "[", 
       RowBox[{"Characters", "[", "cipherText", "]"}], "]"}], "]"}]}]}], 
   "]"}]}]], "Input",
